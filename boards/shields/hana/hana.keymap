#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
                        display-name = "base";
                        bindings = <
                            &kp TAB   &kp Q &kp W &kp E &kp H &kp J
                            &kp LCTRL &kp A &kp S &kp D &kp K &kp L
                        >;
                };
        };
};